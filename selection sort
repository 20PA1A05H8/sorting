#include<stdio.h>
int main(){
    int i,j,n,min,swap;
    printf("enter number of elements:");
    scanf("%d",&n);
    int array[n];
    printf("enter elements:\n");
    for(i=0;i<n;i++)
    scanf("%d",&array[i]);
    for(i=0;i<n-1;i++)
    {
        int min=i;
        for(j=i+1;j<n;j++)
        {
            if(array[j]<array[min])
            min=j;
        }
        if(min!=i)
        {
            swap(array[i],array[min]);
        }
    }
     printf("sorted array:\n");
     for(i=0;i<n;i++)
        printf("%d",array[i]);
    return 0;
}
